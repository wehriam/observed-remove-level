{
  "name": "observed-remove-level",
  "version": "2.2.24",
  "main": "dist/index.js",
  "repository": "git@github.com:wehriam/observed-remove-level",
  "author": "John Wehr <johnwehr@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf ./dist; flow-remove-types ./src --out-dir ./dist && flow-copy-source ./src ./dist",
    "flow-typed": "./node_modules/.bin/flow-typed install --overwrite",
    "test": "yarn lint && yarn test:js",
    "test:js": "cross-env NODE_ENV=test jest tests",
    "lint": "npm run lint:js && npm run lint:flow",
    "lint:js": "eslint -c ./.eslintrc --cache --no-eslintrc --fix .",
    "lint:flow": "flow --show-all-errors",
    "coverage": "codecov",
    "documentation": "documentation readme src/set.js --section='Set API' && documentation readme src/map.js --section='Map API'",
    "start-pre-commit": "git stash -q --keep-index",
    "end-pre-commit": "git add -u && git stash pop -q",
    "profile": "yarn build; clear; node --prof profile/map.js; node --prof-process $(ls isolate-*); rm isolate-*"
  },
  "pre-commit": [
    "lint",
    "start-pre-commit",
    "build",
    "documentation",
    "end-pre-commit"
  ],
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/*.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!observed-remove-level)/"
    ],
    "transform": {
      "^.+\\.js(?:\\.flow)?$": "jest-flow-transform"
    }
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "codecov": "^3.6.1",
    "cross-env": "^6.0.3",
    "documentation": "^12.1.3",
    "eslint": "^6.6.0",
    "eslint-config-bunchtogether": "^1.1.6",
    "eslint-plugin-flowtype": "^4.4.1",
    "eslint-plugin-flowtype-errors": "^4.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.3",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "expect": "^24.9.0",
    "flow-bin": "^0.111.3",
    "flow-copy-source": "^2.0.8",
    "flow-remove-types": "^2.111.3",
    "flow-typed": "^2.6.2",
    "jest": "^24.9.0",
    "jest-flow-transform": "^1.0.1",
    "jest-matcher-utils": "^24.9.0",
    "level": "^6.0.0",
    "pre-commit": "^1.2.2",
    "uuid": "^3.3.3"
  },
  "dependencies": {
    "@bunchtogether/hash-object": "^1.0.3",
    "json-stringify-deterministic": "^1.0.1",
    "node-rsa": "^1.0.6",
    "p-queue": "^6.2.1"
  }
}
